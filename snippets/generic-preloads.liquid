{%- if request.page_type == 'product' -%}
  {%- assign media = product.selected_variant.featured_media | default: product.featured_media -%}
  <link
    rel="preload"
    fetchpriority="high"
    as="image"
    imagesizes="100vw"
    imagesrcset="
      {% if media.preview_image.width >= 360 %}{{ media.preview_image | image_url: width: 360 }} 360w,{% endif %}
      {% if media.preview_image.width >= 540 %}{{ media.preview_image | image_url: width: 540 }} 540w,{% endif %}
      {% if media.preview_image.width >= 720 %}{{ media.preview_image | image_url: width: 720 }} 720w,{% endif %}
      {% if media.preview_image.width >= 900 %}{{ media.preview_image | image_url: width: 900 }} 900w,{% endif %}
      {% if media.preview_image.width >= 1080 %}{{ media.preview_image | image_url: width: 1080 }} 1080w,{% endif %}
      {{ media.preview_image | image_url }} {{ media.preview_image.width }}w
    "
  >
{%- elsif request.page_type == 'index' -%}
  <link
    rel="preload"
    fetchprioty="high"
    as="image"
    href="https://ucarecdn.com/f7a3d568-3632-469b-8b7d-f72c90492a05/-/format/auto/-/preview/3000x3000/-/quality/lighter/mobilebackground.webp"
  >
{%- elsif request.page_type == 'collection' and collection.products[0].featured_image -%}
  <link
    rel="preload"
    fetchpriority="high"
    as="image"
    imagesizes="100vw"
    imagesrcset="
      {%- if collection.products[0].featured_image.width >= 160 -%}{{ collection.products[0].featured_image | image_url: width: 160 }} 160w,{%- endif -%}
      {%- if collection.products[0].featured_image.width >= 200 -%}{{ collection.products[0].featured_image | image_url: width: 200 }} 200w,{%- endif -%}
      {%- if collection.products[0].featured_image.width >= 280 -%}{{ collection.products[0].featured_image | image_url: width: 280 }} 280w,{%- endif -%}
      {%- if collection.products[0].featured_image.width >= 360 -%}{{ collection.products[0].featured_image | image_url: width: 360 }} 360w,{%- endif -%}
      {%- if collection.products[0].featured_image.width >= 540 -%}{{ collection.products[0].featured_image | image_url: width: 540 }} 540w,{%- endif -%}
      {%- if collection.products[0].featured_image.width >= 720 -%}{{ collection.products[0].featured_image | image_url: width: 720 }} 720w,{%- endif -%}
      {%- if collection.products[0].featured_image.width >= 900 -%}{{ collection.products[0].featured_image | image_url: width: 900 }} 900w,{%- endif -%}
      {{ collection.products[0].featured_image | image_url }} {{ collection.products[0].featured_image.width }}w
    "
  >
{%- endif -%}

